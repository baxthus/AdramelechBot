{
  "name": "adramelech-bot",
  "version": "0.0.1",
  "description": "",
  "main": "dist/bot.js",
  "scripts": {
    "start": "doppler run -- tsx src/bot.ts",
    "deploy": "doppler run -- tsx src/deployCommands.ts",
    "build": "tsc && npm run gen-json && cp src/commands.json dist/commands.json && cp src/credits.json dist/credits.json",
    "start:prod": "npm run build && doppler run -- node dist/bot.js",
    "pm2:prod": "npm run build && doppler run -- pm2 start dist/bot.js",
    "lint": "eslint .",
    "docker": "doppler run -- docker-compose up -d",
    "dev": "doppler run -- tsx watch src/bot.ts",
    "gen-json": "tsx src/generateJson.ts"
  },
  "author": "Abysmal",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.7.1",
    "furry-wrapper": "^0.0.5",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "tslib": "^2.4.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@interfaces": "dist/interfaces",
    "@utils": "dist/utils",
    "@commands": "dist/commands",
    "@config": "dist/config.js"
  }
}
